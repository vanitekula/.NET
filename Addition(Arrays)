using System;
					
public class Program
{
	public static void Main()
	{
		int[,] arr={{1,2,3,},{2,3,4}};
        int [,] arr1={{1,2,3},{2,3,4}};
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                Console.Write(arr[i,j]+arr1[i,j]+" ");
            }
            Console.WriteLine();
        }
	}
}
